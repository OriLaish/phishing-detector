{
    "manifest_version": 2,
    "name": "Phishing Detector",
    "version": "1.0",
    "background": 
    {
        "persistent": true,
        "scripts": ["background.html"]
    },
    "permissions" : ["contextMenus" , "webNavigation" , "webRequest", "tabs" ,  "webRequestBlocking", "*://*.youtube.com/*" , "*://*.ynet.co.il/*"],
    "content_scripts":[
        {
            "matches": ["<all_urls>"],
            "js": ["feature_extractor.js"]

        }
    ]
  
    
}